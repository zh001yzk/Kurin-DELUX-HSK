    <thingClass>KurinDeluxeEdition.FabricatorMeatConverter.Building_KurinFabricator</thingClass>
    <graphicData>
      <texPath>Kurin/Building/Fabricator_Various/Texture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3.75,5.75)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
